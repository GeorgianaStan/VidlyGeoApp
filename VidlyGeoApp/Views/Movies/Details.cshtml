@model VidlyGeoApp.Models.Movie
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre</li>
    <li>Release Date: @Model.ReleaseDate.ToString("dddd, MMMM dd, yyyy")</li>
    <li>Genre: @Model.DateAdded.ToString("dddd, MMMM dd, yyyy")</li>
    <li>Genre: @Model.NumberInStock</li>
</ul>
